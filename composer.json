{
    "name": "laravie/query-filter",
    "description": "Database/Eloquent Query Builder filters for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\QueryFilter\\" : "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\QueryFilter\\Tests\\" : "tests/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/database": "^8.25 || ^9.0",
        "illuminate/support": "^8.25 || ^9.0"
    },
    "require-dev": {
        "orchestra/canvas": "^6.1 || ^7.0",
        "orchestra/testbench": "^6.16 || ^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
