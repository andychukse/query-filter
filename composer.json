{
    "name": "laravie/query-filter",
    "description": "Database/Eloquent Query Builder filters for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\QueryFilter\\" : "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\QueryFilter\\Tests\\" : "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.3.2",
        "illuminate/support": "^10.3.2"
    },
    "require-dev": {
        "laravel/pint": "^1.6",
        "nunomaduro/larastan": "^2.6",
        "orchestra/canvas": "^8.1",
        "orchestra/testbench": "^8.5",
        "phpunit/phpunit": "^10.1"
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "ci": [
            "@composer run prepare",
            "@php vendor/bin/phpstan analyse",
            "@php vendor/bin/pint"
        ],
        "test": [
            "@php vendor/bin/phpunit -c ./ --color"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
